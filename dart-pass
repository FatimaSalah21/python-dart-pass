Q1/
//The error is the type of the variable, since it is not possible to write a constant variable to a variable function as the time function//
correct code is:
 main() {
  var hour;
  hour=DateTime.now().hour;
  print(hour);
}

Q2/
abstract class Animal {

  void printName();

  void printSound();

}

class Dog extends Animal{

  void printName(){print("Dog");}

  void printSound(){print("woof");}

}
class Cat extends Animal{
  void printName(){print("Cat");}

  void printSound(){print("Meaw");}

}

class Cow extends Animal{

  void printName(){print("Cow");}

  void printSound(){print("Moo");}
}
void main(){ 
  var cat= Cat(), dog=Dog(), cow=Cow();
  
  cat.printName();
  cat.printSound();
  
  dog.printName();
  dog.printSound();

  cow.printName();
  cow.printSound();
}
